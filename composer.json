{
    "name": "openai/openai-php-client",
    "description": "This is a community-driven, unofficial PHP SDK designed to simplify integration with the OpenAI API.",
    "keywords": [
        "OpenAI",
        "openai sdk",
        "openai-php-client"
    ],
    "type": "metapackage",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/openai-php-client",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "abandoned": "openai-php/client",
    "require": {
        "nyholm/psr7": "^1.8",
        "openai-php/client": "^0.10.3",
        "symfony/http-client": "^6.4"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
