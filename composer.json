{
    "name": "open-ai/openai-php-client",
    "description": "Community-Driven PHP SDK for OpenAi AI API Integration.",
    "keywords": [
        "php",
        "openai",
        "deepseek",
        "qwen",
        "sdk",
        "codex",
        "GPT-3",
        "DALL-E",
        "api",
        "client",
        "natural",
        "language",
        "processing"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/openai-php-client",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "php-http/discovery": "^1.20.0",
        "php-http/multipart-stream-builder": "^1.4.2",
        "psr/http-client": "^1.0.3",
        "psr/http-client-implementation": "^1.0.1",
        "psr/http-factory-implementation": "*",
        "psr/http-message": "^1.1.0|^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAI\\": "openai/src/"
        },
        "files": [
            "openai/src/OpenAI.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
